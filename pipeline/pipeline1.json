{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy Data1",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT \n      [Alpha3] AS Id\n\t  ,[Country_Name]\n      ,[Alpha2]\n      \n      ,[CountryCode]\n      ,[Iso31662]\n      ,[Region]\n      ,[SubRegion]\n      ,[IntermidiateRegion]\n      ,[RegionCode]\n      ,[SubRegionCode]\n  FROM [stg].[Country_Code_Untyped]\nFOR JSON AUTO\n"
					},
					"sink": {
						"type": "DocumentDbCollectionSink",
						"nestingSeparator": ".",
						"writeBehavior": "insert"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "CrimeStatSQLdb",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DocumentDbCollection1",
						"type": "DatasetReference"
					}
				]
			}
		]
	}
}